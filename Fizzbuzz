/*
  Counts numbers between 1-100.
  Outputs Fizz for multiples of 3.
  Outputs Buzz for multiples of 5.
  Outputs FizzBuzz for multiples of 3 and 5.
*/
#include <iostream>

int main() {

  //Brain explodes here:

  for(int i = 1; i <= 100; i++)
  {
  
    //i is divisible by 3 & 5
    if (i % 3 == 0 && i % 5 == 0)
    {
      std::cout << "Fizzbuzz\n";
    }
    
    //i is divisible by 3
    else if (i % 3 == 0)
    {
      std::cout << "Fizz\n";
    }
    
    //i is divisible by 5
    else if (i % 5 == 0)
    {
      std::cout << "Buzz\n";
    }
    
    //numbers not divisible by 3 or 5
    else
    {
      std::cout << i << "\n";
    }
  }

  return 0;

}
