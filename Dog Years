#include <iostream>

int main() {
  
  //age of dog in dog years
  int dog_age = 4;
  
  //first 2 years of a dog's life count as 21 human years
  int early_years = 21;
  
  //each following year counts as 4 human years
  int later_years = (dog_age - 2) * 4;
  
  //calculates dog age in human years
  int human_years = early_years + later_years;
  
  std::cout << "My name is Muffin! Ruff, ruff, I am " << human_years << " years old in human years.\n";
  
  return 0;
  
}
